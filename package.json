{
  "name": "j-templates",
  "version": "2.0.27",
  "description": "j-templates",
  "license": "MIT",
  "repository": "https://github.com/TypesInCode/jTemplates",
  "author": "Jay Landrum",
  "keywords": [
    ""
  ],
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "lint": "tslint --force --format verbose \"src/**/*.ts\"",
    "build": "npm run clean && npm run lint && echo Using TypeScript && tsc --version && tsc --pretty",
    "buildquick": "npm run clean && echo Using TypeScript && tsc --version && tsc --pretty",
    "test": "npm run buildquick && mocha --compilers ts:ts-node/register --recursive test/**/*-spec.ts",
    "watch": "npm run build -- --watch",
    "watch:test": "npm run test -- --watch",
    "webbuild": "npm run clean && webpack --config webpack.dist.config.js",
    "webexamplebuild": "npm run clean && webpack --config webpack.example.config.js",
    "webdevbuild": "npm run clean && webpack",
    "webtest": "npm run clean && webpack && mocha-phantomjs jTemplate.test.html"
  },
  "dependencies": { },
  "devDependencies": {
    "@types/chai": "3.4.30",
    "@types/mocha": "2.2.29",
    "@types/node": "^6.0.117",
    "chai": "^3.5.0",
    "jsdom": "^13.2.0",
    "mocha": "^3.0.1",
    "rimraf": "^2.5.4",
    "ts-loader": "^1.2.0",
    "ts-node": "^6.1.1",
    "tslint": "3.15.1",
    "typescript": "^2.9.2",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "webpack": "^1.15.0"
  },
  "engines": {
    "node": ">=4.0.0"
  }
}
