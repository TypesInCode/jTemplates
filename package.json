  "author": "Jay Landrum",
  "keywords": [
    ""
  ],
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "build": "npm run buildquick && node ./tools/copy-package-json.js && webpack",
    "buildquick": "npm run clean && echo Using TypeScript && tsc --version && tsc --pretty",
    "test": "mocha --require ts-node/register --recursive test/**/*-spec.ts",
    "testcoverage": "nyc  --reporter=html mocha --require ts-node/register --recursive test/**/*-spec.ts",
    "watch": "npm run build -- --watch",
    "watch:test": "npm run test -- --watch",
    "webbuild": "npm run clean && webpack --config webpack.dist.config.js"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "3.4.30",
    "@types/mocha": "2.2.29",
    "@types/node": "^13.1.2",
    "chai": "^3.5.0",
    "fs-extra": "^8.1.0",
    "jsdom": "^13.2.0",
    "mocha": "^7.0.0",
    "nyc": "^15.0.0",
    "rimraf": "^2.5.4",
    "ts-loader": "^7.0.4",
    "ts-node": "^8.10.1",
    "typescript": "^3.7.4",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "engines": {
    "node": ">=4.0.0"
  }
}
